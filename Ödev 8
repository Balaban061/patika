--Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.
UPDATE employee
SET name = 'Sabahattin',
birthday = '1914-07-11',
email = 'sabahattin@ali.com'
WHERE id = 1;
UPDATE employee
SET name = 'Halide Edip',
birthday = '1884-07-11',
email = 'halide@edip.com'
WHERE id = 7;
UPDATE employee
SET name = 'Zygmunt',
birthday = '1911-07-12',
email = 'zygmunt@bauman.com'
WHERE id = 22;
UPDATE employee
SET name = 'Gürkan'
WHERE name LIKE 'A%y';
UPDATE employee
SET email = 'XXXXX'
WHERE birthday = '1992-05-14'
RETURNING *;
--Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM employee
WHERE id = 4;
DELETE FROM employee
WHERE birthday = '1982-10-10';
DELETE FROM employee
WHERE email = 'dwestollk@dot.gov';
DELETE FROM employee
WHERE name = 'Maggy';
DELETE FROM employee
WHERE name LIKE '%o'
RETURNING *;
